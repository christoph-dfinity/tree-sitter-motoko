{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "motoko",
      "camelcase": "Motoko",
      "title": "Motoko",
      "scope": "source.motoko",
      "file-types": [
        "motoko"
      ],
      "injection-regex": "^motoko$",
      "class-name": "TreeSitterMotoko"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "Motoko grammar for tree-sitter",
    "authors": [
      {
        "name": "Christoph Hegemann",
        "email": "christoph.hegemann@dfinity.org"
      }
    ],
    "links": {
      "repository": "https://github.com/christoph-dfinity/tree-sitter-motoko"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
