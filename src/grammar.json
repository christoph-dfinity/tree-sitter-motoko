{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "motoko",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_dec"
              },
              {
                "type": "STRING",
                "value": ";"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_dec"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "doc_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "///"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_text"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "comment_text": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": ".|\\n|\\r"
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z_0-9]*"
    },
    "privileged_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "text_literal": {
      "type": "PATTERN",
      "value": "\"(?:\\\\\"|[^\"])*\""
    },
    "char_literal": {
      "type": "PATTERN",
      "value": "'\\\\''|'[^']*'"
    },
    "float_literal": {
      "type": "PATTERN",
      "value": "[+-]?\\d+\\.\\d+"
    },
    "int_literal": {
      "type": "PATTERN",
      "value": "[+-]?[0-9_]+"
    },
    "hex_literal": {
      "type": "PATTERN",
      "value": "[+-]?0x[0-9a-fA-F_]+"
    },
    "bool_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null_literal": {
      "type": "STRING",
      "value": "null"
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text_literal"
        },
        {
          "type": "SYMBOL",
          "name": "char_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        },
        {
          "type": "SYMBOL",
          "name": "int_literal"
        },
        {
          "type": "SYMBOL",
          "name": "hex_literal"
        },
        {
          "type": "SYMBOL",
          "name": "bool_literal"
        },
        {
          "type": "SYMBOL",
          "name": "null_literal"
        }
      ]
    },
    "_dec": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_exp"
        },
        {
          "type": "SYMBOL",
          "name": "let_dec"
        },
        {
          "type": "SYMBOL",
          "name": "let_else_dec"
        }
      ]
    },
    "let_dec": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "let"
        },
        {
          "type": "SYMBOL",
          "name": "_pat"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_exp"
        }
      ]
    },
    "let_else_dec": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "let_dec"
        },
        {
          "type": "STRING",
          "value": "else"
        },
        {
          "type": "SYMBOL",
          "name": "_exp"
        }
      ]
    },
    "_exp": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_literal"
        },
        {
          "type": "SYMBOL",
          "name": "var_exp"
        }
      ]
    },
    "var_exp": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "var_ty"
        }
      ]
    },
    "var_ty": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "_pat": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "var_pat"
        }
      ]
    },
    "var_pat": {
      "type": "SYMBOL",
      "name": "identifier"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "doc_comment"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}